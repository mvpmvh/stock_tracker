<article class="stock-detail">
    <h3>{{stock.name}}</h3>
    <div class="detail">
        <span>Symbol:</span> {{stock.symbol}}
    </div>
    <div class="detail">
        <span>Low:</span> {{stock.low}}
    </div>
    <div class="detail">
        <span>High:</span> {{stock.high}}
    </div>
    <div class="detail">
        <span>Open:</span> {{stock.open}}
    </div>
    <div class="detail">
        <span>Close:</span> {{stock.close}}
    </div>
</article>